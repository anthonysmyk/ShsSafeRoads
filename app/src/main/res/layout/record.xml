<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back">
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="@dimen/_70sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_65sdp"
        android:adjustViewBounds="true"
        android:background="@drawable/carzz" />

    <ImageView
        android:id="@+id/dtime"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_30sdp"
        android:background="@drawable/ic_log"
        android:ems="10"
        android:gravity="center"
        android:hint=""
        android:inputType="textPersonName" />

    <ImageView
        android:layout_width="@dimen/_222sdp"
        android:layout_height="@dimen/_270sdp"
        android:layout_marginTop="@dimen/_140sdp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/ic_form1" />


    <Spinner
        android:id="@+id/password"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_241sdp"
        android:background="@null"
        android:ems="10"
        android:gravity="center"
        android:hint=""
        android:inputType="textPersonName" />
    <EditText
        android:id="@+id/minutes"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_300sdp"
        android:background="@null"
        android:ems="10"
        android:gravity="center"
        android:hint=""
        android:inputType="textPersonName" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_360sdp"
        android:background="@null"
        android:ems="10"
        android:gravity="center"
        android:hint=""
        android:inputType="textPersonName"
        android:spinnerMode="dialog"


        />


    <Button
        android:id="@+id/button"
        android:layout_width="@dimen/_140sdp"
        android:layout_height="@dimen/_67sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/_80sdp"
        android:layout_marginTop="@dimen/_415sdp"
        android:background="@drawable/ic_save"
        android:onClick="login" />


    <EditText
        android:id="@+id/editText1"
        android:layout_width="@dimen/_180sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_178sdp"
        android:background="@null"
        android:ems="10"
        android:gravity="center"
        android:hint=""
        android:inputType="textPersonName"  />

</RelativeLayout>